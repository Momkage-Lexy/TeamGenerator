@model TeamGenerator2.Models.Names

@{
    ViewData["Title"] = "Groups";
}

<h2>Submitted Names and Team Size</h2>

<p><strong>Team Size:</strong> @Model.TeamSize</p>

<ul>
@foreach (var name in Model.Name)
{
    <li>@name</li>
}
</ul>

@for (int i = 0; i < Model.TeamSize; i++)
{
    <div><h1>Hello</h1></div>
}